{
    "manifest_version": 3,
    "name": "ProtonMailCounter",
    "version": "0.1.3",
    "description": "Unofficial Proton Mail counter extension for chrome",
    "action": {
        "default_icon": {
            "16": "icon/icon16.png",
            "19": "icon/icon19.png",
            "32": "icon/icon32.png",
            "38": "icon/icon38.png",
            "48": "icon/icon48.png",
            "128": "icon/icon128.png"
        }
    },
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": [
        "tabs",
        "storage",
        "alarms"
    ],
    "icons": {
        "16": "icon/icon16.png",
        "19": "icon/icon19.png",
        "32": "icon/icon32.png",
        "38": "icon/icon38.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    }
}